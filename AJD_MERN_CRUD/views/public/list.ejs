
<!DOCTYPE html>
  <head>
    <title>list</title>
    <link rel="icon" type="image/x-icon" href="./assets/favicon.ico" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
  </head>

  <body>
    <h3>
        <a class="btn btn-secondary" href="/">
           <i class="fa fa-plus"></i>
            Create New</a> 
        </h3>
        <h3 class="px-20">{{viewTitle}}</h3>
   <table class="table table-striped table-responsive ">
       <thead>
           <tr>
               <th>Name</th>
               <th>Email</th>
               <th>phone</th>
               <th>Edit</th>
               <th>Delete</th>
             
               <th></th>
           </tr>
       </thead>
       <tbody>
           {{#each List}}
   
           <tr>
               <td>{{this.fullname}}</td>
               <td>{{this.email}}</td>
               <td>{{this.phone}}</td> 
               
            
              <td> <a href="/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a></td>
               <td>
                  
                   <a href="/delete/{{this._id}}" ><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
               </td>
               <!-- onclick="return confirm('Are you sure to delete this record ?');" -->
           </tr>
           {{/each}}
       </tbody>
   </table>
   
   
   
  </body>
</html>